                                                                </thead>
                                                                <tbody class="table-body">
                                                                    @{
                                                                        int i = 1;
                                                                        string alert;
                                                                        string alert_text;
                                                                    @foreach (ModuloGPS d in Model)
                                                                    {
                                                                        float TEMP = float.Parse(d.Temperatura);
                                                                        if(TEMP>=35){ alert = "success"; alert_text = "ACEPTABLE"; }else{ alert = "danger"; alert_text = "ALERTA";}
                                                                    <tr class="cell-1" data-toggle="collapse" data-target="@($"#demo-{i}")">
                                                                        <td class="text-center">@d.IdCond</td>
                                                                        <td>@d.IdLin</td>
                                                                        <td>@d.Placas</td>
                                                                        <td><span class="@($"badge text-bg-{alert}")">@($"{alert_text}")</span>&nbsp;&nbsp;@($"{@d.Temperatura} °C")</td> 
                                                                        <td>@d.Fecha,@d.Hora</td>
                                                                        <td>@d.Direccion</td>
                                                                        <td class="table-elipse" data-toggle="collapse" data-target="#demo"><i class="fa fa-ellipsis-h text-black-50"></i></td>
                                                                    </tr>
                                                                    <tr id="@($"demo-{i}")" class="collapse cell-1 row-child">
