                                                                        <td colspan="7"> <div><partial name="_ChartPartial" /></div> </td> 
                                                                        <script>
                                                                        document.getElementById('chart').id = '@($"chart-{i}")';
                                                                        </script>
                                                                    </tr>
                                                                    i++;
