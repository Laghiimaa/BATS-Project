                                                                <table class="table">
                                                                    <thead>
                                                                        <tr>
                                                                        <th class="text-center">IdCond</th>
                                                                        <th>IdLin</th>
                                                                        <th>Placas</th>
                                                                        <th>Temperatura</th>
                                                                        <th>U. Actualización</th>
                                                                        <th>Dirección</th>
                                                                        <th></th>
